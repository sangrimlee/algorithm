{
  "name": "@algorithm/cli",
  "version": "1.0.3",
  "private": true,
  "description": "Template generation cli",
  "type": "module",
  "bin": {
    "algorithm": "./bin/algorithm.js",
    "markdown": "./bin/markdown.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf .turbo dist node_modules",
    "dev": "tsup --watch",
    "format": "prettier . --check --cache --ignore-path ../../.gitignore",
    "lint": "eslint --cache",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@algorithm/prettier-config",
  "dependencies": {
    "axios": "^1.8.2",
    "cheerio": "^1.0.0",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "glob": "^11.0.1",
    "inquirer": "12.4.3",
    "prettier": "catalog:",
    "ts-dedent": "^2.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@algorithm/eslint-config": "workspace:*",
    "@algorithm/prettier-config": "workspace:*",
    "@algorithm/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "tsup": "^8.4.0",
    "typescript": "catalog:"
  }
}
