{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "cache": true
    },
    "lint": {
      "dependsOn": ["^build", "^lint"],
      "cache": true
    },
    "test": {
      "outputs": []
    },
    "test:watch": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^build", "^typecheck"],
      "cache": true
    }
  }
}
